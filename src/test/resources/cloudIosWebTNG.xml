<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Cloud ios web mobile testing Suite">
    <parameter name="appType" value="web"></parameter>
    <parameter name="udid" value="00008030-000564523AF8802E"></parameter>
    <parameter name="platformName" value="iOS"></parameter>
    <parameter name="browserName" value="Safari"></parameter>
    <parameter name="hubType" value="cloud"></parameter>

    <test name="Web tests">
        <groups>
            <run>
                <include name="web"/>
            </run>
        </groups>
        <packages>
            <package name="scenarios"/>
        </packages>
    </test>
</suite>
